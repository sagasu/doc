module.exports = (robot) ->

  # Enable a looser regex if environment variable is set
  if process.env.HUBOT_SHIP_EXTRA_CAPYBARAS
    regex = /kaizen(ing|ed)?\s*it/i
  else
    regex = /kaizen\s*it/i

  robot.hear regex, (msg) ->
    msg.send msg.random capybaras
